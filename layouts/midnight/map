# console keymap for the midnight layout
# Public domain.

alt_is_meta

keymaps 0-12
include "qwerty-layout"
include "linux-with-alt-and-altgr"
strings as usual

keycode  41  = nul
keycode  2   = Left
keycode  3   = Right
keycode  4   = Control
keycode  5   = percent       percent      braceright
keycode  6   = 0x2013
keycode  7   = nul
keycode  8   = nul
keycode  9   = 0x2026
keycode  10  = 0x2014        bracketright bracketright
keycode  11  = Control
keycode  12  = bar           parenright   parenright
keycode  13  = 0x29BF

keycode  16  =  q             Q            exclam
keycode  17  =  b             B            braceleft
keycode  18  =  f             F            bracketleft
keycode  19  =  l             L            numbersign
keycode  20  =  k             K            equal
keycode  21  =  Escape        Compose         Compose
keycode  22  =  p             P            asciicircum
keycode  23  =  g             G            at
keycode  24  =  o             O            parenleft
keycode  25  =  u             U            dollar
keycode  26  =  period        question     grave
keycode  27  =  colon         asciitilde
keycode  43  =  Insert

keycode  30  =  n             N            1
